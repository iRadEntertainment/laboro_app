	var filtri = dati.fin_contabilita.filtri_contanti
	if true in filtri:  $ico1.show()
	else:               $ico1.hide()
	
	if false in filtri: $ico2.show()
	else:               $ico2.hide()

func _btn_click():
	var filtri = dati.fin_contabilita.filtri_contanti
	if true in filtri and false in filtri:
		dati.fin_contabilita.filtri_contanti = [true]
	elif true in filtri:
		dati.fin_contabilita.filtri_contanti = [false]
	else:
		dati.fin_contabilita.filtri_contanti = [true,false]
	check_status()
	dati.fin_contabilita.applica_filtri_a_lista()"
