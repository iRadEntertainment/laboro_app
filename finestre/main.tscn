func compila_box_preventivo(testo):
#------ clear voci
	for child_id in range(2, (n_box_prev.get_child_count() - 5) ):
		n_box_prev.get_child(child_id).queue_free()
#------ costo_metalli
	var materiale     = dati.diz_preventivi_prezzi[\"metalli\"][op_metallo.get_selected()][1]
	var peso          = float(n_ed_peso.get_text())
	var perc_perdita  = float(n_ed_perd_mat.get_text())/100 + 1.0
	var costo_metalli = utl.rendi_decimale2str(materiale * peso * perc_perdita)
	if costo_metalli != \" - \": aggiungi_voce_anteprima(\"Metalli\",costo_metalli)
#------ costo_semilav
	var costo_semilav = utl.rendi_decimale2str(n_ed_semil.get_text())
	if costo_semilav != \" - \": aggiungi_voce_anteprima(\"Semilavorati\",costo_semilav)
#------ costo_pietre
	var costo_pietre = utl.rendi_decimale2str(n_ed_pietre.get_text())
	if costo_pietre != \" - \": aggiungi_voce_anteprima(\"Pietre varie\",costo_pietre)
#------ lista_pietre
	var pietre_selezionate = []
	for figlio in n_vbox_pietre.get_children():
		var pietra_nome      = dati.diz_preventivi_prezzi[\"pietre\"][figlio.get_node(\"op_pietra\").get_selected()][0]
		var pietra_costo     = dati.diz_preventivi_prezzi[\"pietre\"][figlio.get_node(\"op_pietra\").get_selected()][1]
		var pietra_quantita  = float( figlio.get_node(\"ed_quantita\").get_text() )
		var pietra_costo_tot = pietra_costo * pietra_quantita
		pietre_selezionate.append([pietra_nome,pietra_costo,pietra_quantita,pietra_costo_tot])
		pietra_costo_tot = utl.rendi_decimale2str(pietra_costo_tot)
		aggiungi_voce_anteprima(str(pietra_quantita,\"x \",pietra_nome) , pietra_costo_tot)
#------ costo_lavorazione
	var tipo_lavoro       = dati.diz_preventivi_prezzi[\"tipo\"][op_tipo.get_selected()][1]
	var tempo_stimato     = float(n_ed_tempo.get_text())
	var complessita       = dati.diz_preventivi_prezzi[\"difficolta\"][op_diff.get_selected()][1]
	var costo_lavorazione = utl.rendi_decimale2str(tipo_lavoro * tempo_stimato * complessita)
	if costo_lavorazione != \" - \": aggiungi_voce_anteprima(\"Lavorazione\",costo_lavorazione)
#------ costo_pacchetto
	var costo_pacchetto = utl.rendi_decimale2str(dati.diz_preventivi_prezzi[\"pacchetti\"][op_confez.get_selected()][1]*int(fl_confez))
	if costo_pacchetto != \" - \": aggiungi_voce_anteprima(\"Costi fissi\",costo_pacchetto)
#------ costo_sub_totale
	var costo_tot_pietre_lista = 0
	for pietre in pietre_selezionate:
		costo_tot_pietre_lista += pietre[3]
	var costo_sub_totale =  utl.rendi_decimale2str(float(costo_metalli)+float(costo_semilav)\\
							+float(costo_pietre)+float(costo_lavorazione)+float(costo_pacchetto)+\\
							costo_tot_pietre_lista)
	pr_subtot.set_text(costo_sub_totale)
#------ costo_totale
	var costo_totale = utl.rendi_decimale2str( float(costo_sub_totale) * ( 1.0+float(dati.diz_preventivi_prezzi[\"IVA\"])/100 ) )
	pr_tot.get_parent().get_node(\"lb_tot\").set_text(\"IVA \"+str(dati.diz_preventivi_prezzi[\"IVA\"])+\"% - Totale\")
	pr_tot.set_text(costo_totale)
#------ sconto e acconto
	n_ed_acconto.set_placeholder(utl.rendi_decimale2str(float(costo_totale)/5))
	var sconto          = int( float(n_ed_sconto.text)*10 )/10
	var sconto_in_euro  = utl.rendi_decimale2str(( float(n_ed_sconto.text)/100 * float(costo_totale) ))
	var costo_scontato  = utl.rendi_decimale2str( float(costo_totale) - float(sconto_in_euro) )
	var acconto         = utl.rendi_decimale2str( float(n_ed_acconto.get_text()) )
	var resto_da_pagare = utl.rendi_decimale2str( float(costo_scontato) - float(acconto) )
	if float(n_ed_acconto.get_text()) == 0 and sconto == 0:
		pr_resto.get_parent().hide()
	else:
		pr_resto.get_parent().show()
		pr_resto.get_parent().get_node(\"lb_acconto\").set_text(\"Sconto \"+str(sconto)+\"%: \"+sconto_in_euro+\" - Acconto: \"+acconto+\" - Da pagare:\")
		pr_resto.set_text(resto_da_pagare)
#------ guadagno
	var val_costi = utl.rendi_decimale2str( float(costo_pacchetto)+ float(costo_pietre) +\\
										float(costo_semilav) + float(costo_metalli) +\\
										float(costo_totale)- float(costo_sub_totale) )
	var val_guadagni = utl.rendi_decimale2str( float(costo_scontato) - float(val_costi) )
	var str_costo_guadagno = \"Costi:\" + val_costi + \" - Netto: \" + val_guadagni
	lb_guadagno.set_text(str_costo_guadagno)
#------ finalizza
	preventivo_costi = [ costo_metalli , costo_semilav,        # 0 1
	costo_pietre , pietre_selezionate , costo_lavorazione,     # 2 3 4
	costo_pacchetto , costo_sub_totale , costo_totale,         # 5 6 7
	acconto, sconto] 
	#ID:3 ---- pietre_selezionate = [pietra_nome , pietra_costo , pietra_quantita , pietra_costo_tot]
	n_box_prev.hide() ; yield(get_tree(),\"idle_frame\") ; n_box_prev.show()


