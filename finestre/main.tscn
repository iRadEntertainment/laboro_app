script/source = "#btn_opzioni

extends TextureButton

onready var n_tween        = get_node(\"logo_anim\")
onready var n_loghino      = get_node(\"../loghino\")

func _ready():
	connect(\"pressed\",self,\"_click\")
	connect(\"mouse_entered\",self,\"_ingrandisci_logo\")
	connect(\"mouse_exited\",self,\"_riduci_logo\")

func _click():
	if !dati.fin_settaggi.is_visible(): dati.fin_settaggi.show()
	else:                               dati.fin_settaggi.hide()
	n_loghino.scale = Vector2(0.95,0.95) ; _ingrandisci_logo()

func _ingrandisci_logo():
	n_tween.stop_all()
	n_tween.interpolate_property(n_loghino, \"scale\", n_loghino.scale, Vector2(1.05,1.05), 0.2, Tween.TRANS_QUAD, Tween.EASE_IN)
	n_tween.start()

func _riduci_logo():
	n_tween.stop_all()
	n_tween.interpolate_property(n_loghino, \"scale\", n_loghino.scale, Vector2(1,1), 0.3, Tween.TRANS_QUAD, Tween.EASE_IN)
	n_tween.start()"

