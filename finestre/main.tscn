resource_name = "Main"
script/source = "#MAIN

extends Container

onready var tutte_le_fin = [$barra_centro/fin_turni,\\
	                $barra_centro/fin_rubrica,\\
	                $barra_centro/fin_preventivo,\\
					$barra_centro/fin_contabilita]

func _ready():
	inizializza_settaggi()
	set_process_input(true)

func _input(event):
	#se si preme ESC
	if event.is_action_pressed(\"ui_cancel\"):
		var pop_esci = get_node(\"cnt_popups/pop_esci\")
		if OS.is_window_fullscreen(): OS.set_window_fullscreen(false)
		elif pop_esci.visible: pop_esci.hide()
		elif not pop_esci.visible: pop_esci.show()

func inizializza_settaggi():
	if dati.fl_fullscreen:  OS.set_window_fullscreen(true)
	if dati.fl_low_process: OS.set_low_processor_usage_mode(true)
	
	dati.fin_orologio.visible = dati.fl_orologio
	mostra_fin(dati.fl_mostra_fin)
	$aspetta.start()

func aggiorna_tutto():
#	------- aggiorna fin_turni
	dati.fin_turni.compila_tabelle()
	dati.fin_turni.calcola_ore()
	dati.fin_turni.aggiorna_sunto()
#	------- aggiorna fin_rubrica
	dati.fin_rubrica.compila_rubrica()
	dati.fin_rubrica.imposta_btn_iniz()
#	------- aggiorna fin_preventivo
	dati.fin_preventivo.riaggiorna_tutto()

func mostra_fin(fin_num):
	tutte_le_fin = [$barra_centro/fin_turni,\\
	                $barra_centro/fin_rubrica,\\
	                $barra_centro/fin_preventivo,\\
					$barra_centro/fin_contabilita]
	for finestra in tutte_le_fin: finestra.hide()
	
	if   fin_num == 21:
		dati.fin_preventivo.show()
		dati.fin_preventivo.get_node(\"cnt_lista_prev\").hide()
		dati.fin_preventivo.get_node(\"cnt_nuovo_prev\").show()
	elif fin_num == 22:
		dati.fin_preventivo.show()
		dati.fin_preventivo.get_node(\"cnt_lista_prev\").show()
		dati.fin_preventivo.get_node(\"cnt_nuovo_prev\").hide()
	else: tutte_le_fin[fin_num].show()
	
	var linea_anim = get_node(\"barra_sotto/linea_anim\")
	linea_anim.anima_linea(fin_num)
	"

