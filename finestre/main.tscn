	print(\"tentativo di caricare i dati da fin_turni\")

func salva_dati():
	print(\"tentativo di salvare i dati dei turni da fin_turni\")
#	dati.salva_dati(\"turni\")

func cambia_finestra(num):
	if $anim.is_playing():
		yield($anim,\"animation_finished\")
	if   fin_aperta == 0 and num == 1:
		$anim.play(\"d0a1\") ; fin_aperta = num
	elif fin_aperta == 1 and num == 0:
		$anim.play(\"d1a0\") ; fin_aperta = num
	elif fin_aperta == 1 and num == 2:
		$anim.play(\"d1a2\") ; fin_aperta = num
	elif fin_aperta == 2 and num == 1:
		$anim.play(\"d2a1\") ; fin_aperta = num
	
	if   fin_aperta == 0: n_testo_sett.text = \"Settimana passata - \"  + dati.settimana_prec
	elif fin_aperta == 1: n_testo_sett.text = \"Settimana corrente - \" + dati.settimana_corr
	elif fin_aperta == 2: n_testo_sett.text = \"Settimana prossima - \" + dati.settimana_succ
	aggiorna_sunto()

func aggiorna_sunto():
	var ore_da_mostrare
	if   fin_aperta == 0: ore_da_mostrare = ore_pass
	elif fin_aperta == 1: ore_da_mostrare = ore_corr
	elif fin_aperta == 2: ore_da_mostrare = ore_pros
	
	var sunto = get_node(\"h_box_btn/sunto\")
	sunto.get_node(\"Marco\").text     = \"Marco: \"    +str(ore_da_mostrare[0])+\" ore\"
	sunto.get_node(\"Francesca\").text = \"Francesca: \"+str(ore_da_mostrare[1])+\" ore\"
	sunto.get_node(\"Federica\").text  = \"Federica: \" +str(ore_da_mostrare[2])+\" ore\""

