func calcola_saldo():
	tot_spese = [0,0,0]
	tot_guad  = [0,0,0]
	tot       = [0,0,0]
	tot_cash  = [0,0,0]
	tot_bank  = [0,0,0]
	
	var anno_corrente = OS.get_date()[\"year\"]
	var mese_corrente = OS.get_date()[\"month\"]
	
	var anno_scorso = OS.get_date()[\"year\"]
	var mese_scorso = mese_corrente -1
	if mese_scorso == 0:
		mese_scorso = 12
		anno_scorso -= 1
	
	for voce in dati.diz_contabilita:
		var mese = OS.get_datetime_from_unix_time(voce[1][\"data\"])[\"month\"]
		var anno = OS.get_datetime_from_unix_time(voce[1][\"data\"])[\"year\"]
		
		if voce[1][\"valore\"] < 0:
			tot_spese[0] += voce[1][\"valore\"]
			if   mese == mese_corrente and anno == anno_corrente:
				tot_spese[1] += voce[1][\"valore\"]
			elif mese == mese_scorso and anno == anno_scorso:
				tot_spese[2] += voce[1][\"valore\"]
		else:
			tot_guad[0]  += voce[1][\"valore\"]
			if   mese == mese_corrente and anno == anno_corrente:
				tot_guad[1] += voce[1][\"valore\"]
			elif mese == mese_scorso and anno == anno_scorso:
				tot_guad[2] += voce[1][\"valore\"]
			
		
		if voce[1][\"contanti\"]:
			tot_cash[0]  += voce[1][\"valore\"]
			if   mese == mese_corrente and anno == anno_corrente:
				tot_cash[1] += voce[1][\"valore\"]
			elif mese == mese_scorso and anno == anno_scorso:
				tot_cash[2] += voce[1][\"valore\"]
		else:
			tot_bank[0]  += voce[1][\"valore\"]
			if   mese == mese_corrente and anno == anno_corrente:
				tot_bank[1] += voce[1][\"valore\"]
			elif mese == mese_scorso and anno == anno_scorso:
				tot_bank[2] += voce[1][\"valore\"]
	
	tot[0] = tot_spese[0] + tot_guad[0]
	tot[1] = tot_spese[1] + tot_guad[1]
	tot[2] = tot_spese[2] + tot_guad[2]
	
	var stato_btn = $body/body1/box_saldo/btn_saldo.stato
	compila_box_saldo(stato_btn)

