var filtri_contanti = [true,false]
var filtri_descr    = \"\"
#-------- saldo
var tot_spese = [0,0,0]
var tot_guad  = [0,0,0]
var tot       = [0,0,0]
var tot_cash  = [0,0,0]
var tot_bank  = [0,0,0]

onready var voce_preload = preload(\"res://finestre/voce_contabilita.tscn\")

onready var ico_10  = preload(\"res://sprite/cont_ico_10.png\")
onready var ico_10w = preload(\"res://sprite/cont_ico_10w.png\")
onready var ico_11  = preload(\"res://sprite/cont_ico_11.png\")
