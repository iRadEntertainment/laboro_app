onready var n_box_stima      = get_node(\"cnt_nuovo_prev/cnt_centro/scr_stima/box_stima\")
onready var n_ed_peso        = get_node(\"cnt_nuovo_prev/cnt_centro/scr_stima/box_stima/lb_peso/ed_peso\")
onready var n_ed_perd_mat    = get_node(\"cnt_nuovo_prev/cnt_centro/scr_stima/box_stima/lb_perd_mat/ed_perd_mat\")
