onready var ico_33  = preload(\"res://sprite/cont_ico_33.png\")
onready var ico_33w = preload(\"res://sprite/cont_ico_33w.png\")


func _ready():
	imposta_data_nuova_voce()
	imposta_data_filtri()
	compila_op_tipo(1)
	aggiorna_lista_contabilita()
	calcola_saldo()

func salva_nuova_voce():
	var voce_temp = []
	var n_voce_temp = dati.diz_contabilita.size()+1
#	prints(\"N_VOCE:\",n_voce_temp)
	prints(\"ID_TEMP:\",id_temp)
	var cash_temp = $barra_sopra/nuova_voce/op_cash.get_selected_id() == 0
	prints(\"CASH:\",cash_temp)
	var val_temp  = abs(float($barra_sopra/nuova_voce/val.text))
	if id_temp in id_op_pagamenti or id_temp in [31,33]:
		val_temp = -val_temp
	prints(\"VALORE:\",val_temp)
	var descr_temp = $barra_sopra/nuova_voce/desc.text
	prints(\"DESCR:\",descr_temp)
	
	var cliente_temp = \"\"
	if id_temp in id_op_incassi:
		cliente_temp = $barra_sopra/nuova_voce/cliente.text
		if cliente_temp != \"\":
			if cliente_temp in dati.lista_nomi:
				dati.fin_rubrica.aggiungi_contabilita_a_contatto(cliente_temp,n_voce_temp)
			else:
				dati.fin_rubrica.aggiungi_contatto_da_contabilita(cliente_temp,n_voce_temp)
	
	var unix_temp = {\"second\": 1,
					\"day\"    : int($barra_sopra/nuova_voce/ed_giorno.text),
					\"month\"  :     $barra_sopra/nuova_voce/op_mese.get_selected_id(),
