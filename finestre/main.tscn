script/source = "#btn_set_installa_office

extends Button

onready var png_ok = preload(\"res://sprite/smb_ok.png\")

func _ready():
	visible = dati.fl_debug_mode
	connect(\"pressed\",self,\"_click\")
	controlla_inst_soffice()

func _click():
	if dati.fl_soffice_inst: return
	if OS.get_name() != \"Windows\":
		OS.execute(\"apt-get\",[\"install\",\"soffice\"],true)
	controlla_inst_soffice()

func controlla_inst_soffice():
	var comando
	if OS.get_name() != \"Windows\": comando = \"which\"
	else:                          comando = \"where\"
	
	var uscita_cmd = []
	OS.execute(\"where\",[\"soffice\"],true,uscita_cmd)
	
	var test_win = uscita_cmd[0].find(\"\\\\soffice\")   #risultato = -1 se sub-string non trovata
	var test_osx = uscita_cmd[0].find(\"bin/soffice\")
	if test_win != -1 or test_osx != -1:
		text = \"Libre Office: OK\"
		$attenzione.texture = png_ok
		if not dati.fl_soffice_inst:
			dati.fl_soffice_inst = true
			dati.salva_dati(\"settaggi\")
	else:
		if dati.fl_soffice_inst:
			dati.fl_soffice_inst = false
			dati.salva_dati(\"settaggi\")"

