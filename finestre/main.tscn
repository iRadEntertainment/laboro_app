[sub_resource type="GDScript" id=39]

resource_name = "fin_rubrica"
script/source = "#fin_rubrica

extends Container

onready var n_btn_salva    = get_node(\"h_box/pan_note/h_box_btns/btn_salva\")
onready var n_btn_aggiungi = get_node(\"h_box/pan_note/h_box_btns/btn_aggiungi\")
onready var n_btn_annulla  = get_node(\"h_box/pan_note/h_box_btns/btn_annulla\")
onready var n_btn_modifica = get_node(\"h_box/pan_note/h_box_btns/btn_modifica\")
onready var n_btn_elimina  = get_node(\"h_box/pan_note/h_box_btns/btn_elimina\")
onready var pop_avviso     = get_node(\"cont_pop/pop_avviso\")
onready var pop_conferma   = get_node(\"cont_pop/pop_conferma\")

onready var n_sfondo       = get_node(\"h_box/pan_note/sfondo\")
onready var n_riga1        = get_node(\"h_box/pan_note/riga1\")
onready var n_riga2        = get_node(\"h_box/pan_note/riga2\")
var fl_mod_aggiungi = false
var fl_mod_modifica = false
var nome_da_mod

onready var n_ico       = get_node(\"h_box/pan_note/riga1/ico\")
onready var n_nome      = get_node(\"h_box/pan_note/riga1/nome\")
onready var n_d_nasc    = get_node(\"h_box/pan_note/riga1/d_nasc\")
onready var n_email     = get_node(\"h_box/pan_note/riga2/email\")
onready var n_tel       = get_node(\"h_box/pan_note/riga2/tel\")
onready var n_note      = get_node(\"h_box/pan_note/note\")

onready var n_barra_ric = get_node(\"h_box/pan_contatti/ed_cerca\")
onready var n_lista     = get_node(\"h_box/pan_contatti/cnt_lista/lista\")

onready var text_pers   = preload(\"res://sprite/rubr_ico_contact.png\")
onready var text_forn   = preload(\"res://sprite/rubr_ico_shop.png\")
onready var immaginetta = [text_pers, text_forn]
onready var inst_voce   = preload(\"res://finestre/voce_rubrica.tscn\")

func _ready():
#	yield(dati,\"tutte_var_global_caricate\")
	compila_rubrica()
	svuota_pannello_note()
	imposta_btn_iniz()

func salva_rubrica():
	print(\"DATI: fin_rubrica -> Tentativo di accessio a salva_rubrica()\")
	dati.salva_dati(\"rubrica\")

func compila_rubrica():
	for id_child in range( n_lista.get_child_count() ): #SVUOTA LISTA
		n_lista.get_child(id_child).queue_free()
	
	dati.lista_nomi.sort()
	for chiave in dati.lista_nomi:
		var id_imm     = dati.diz_contatti[chiave][\"ico\"]
		var nuova_voce = inst_voce.instance()
		
		n_lista.add_child( nuova_voce )
		nuova_voce.set_name(chiave)
		nuova_voce.var_nome = chiave
		nuova_voce.var_tel  = dati.diz_contatti[chiave][\"tel\"]
		nuova_voce.get_node(\"nome\").set_text(chiave)
		nuova_voce.get_node(\"tel\").set_text(dati.diz_contatti[chiave][\"tel\"])
		nuova_voce.get_node(\"ico\").set_texture(immaginetta[id_imm])

#{ nome_cognome: {\"ico\":ico, \"d_nasc\":d_nasc, \"mail\":mail, \"tel\":tel, \"note\":note,
#                 \"acqu_effett\":[1,2,34,55,..], \"prev_effett\":[\"Pr-00001\", \"Pr-00002\",...]}, ... }

func aggiungi_contatto_a_dati():
	var nome   = n_nome.get_text()
	var ico    = n_ico.get_selected_id()
	#n_ico.get_selected_ID()
	var d_nasc = n_d_nasc.get_text()
	var mail   = n_email.get_text()
	var tel    = n_tel.get_text()
	var note   = n_note.get_text()
	if dati.diz_contatti.has(nome):
		if not (dati.diz_contatti[nome][\"d_nasc\"] == d_nasc and dati.diz_contatti[nome][\"tel\"] == tel):
			pop_avviso.set_title(\"Omonimo\")
			pop_avviso.set_text(\"Scegliere un nome diverso\")
			pop_avviso.popup()
			return
		else:
			pop_avviso.set_title(\"Attenzione\")
			pop_avviso.set_text(\"Contatto già esistente\")
			pop_avviso.popup()
			return
	dati.diz_contatti[nome] = {\"ico\":ico, \"d_nasc\":d_nasc, \"mail\":mail, \"tel\":tel, \"note\":note}
	if not dati.lista_nomi.has(nome):
		dati.lista_nomi.append(nome)
		dati.lista_nomi_aggiornata()
	compila_rubrica()
	svuota_pannello_note()
	dati.salva_dati(\"rubrica\")


#------------aggiungi contatto dalla fin_preventivi
func aggiungi_contatto_da_preventivi(pr_nome,pr_tel,pr_mail,pr_num):      #<---da fin_preventivi
	dati.lista_nomi.append(pr_nome)
	dati.diz_contatti[pr_nome] = {\"ico\":0, \"d_nasc\":\"\", \"mail\":pr_mail, \"tel\":pr_tel, \"note\":\"\", \"prev_effett\":[pr_num]}
	dati.lista_nomi_aggiornata()
	dati.salva_dati(\"rubrica\")
	compila_rubrica()

func aggiungi_preventivo_a_contatto(pr_nome,pr_num):       #<---da fin_preventivi
	if dati.diz_contatti[pr_nome].has(\"prev_effett\"):
		dati.diz_contatti[pr_nome][\"prev_effett\"].append(pr_num)
	else:
		dati.diz_contatti[pr_nome][\"prev_effett\"] = [pr_num]
	dati.salva_dati(\"rubrica\")

#------------aggiungi contatto dalla fin_contabilita
func aggiungi_contatto_da_contabilita(pr_nome,pr_num):      #<---da fin_contabilita
	dati.lista_nomi.append(pr_nome)
	dati.diz_contatti[pr_nome] = {\"ico\":0, \"d_nasc\":\"\", \"mail\":\"\", \"tel\":\"\", \"note\":\"\", \"acqu_effett\":[pr_num]}
	dati.salva_dati(\"rubrica\")
	dati.lista_nomi_aggiornata()
	compila_rubrica()

func aggiungi_contabilita_a_contatto(pr_nome,pr_num):       #<---da fin_contabilita
	if dati.diz_contatti[pr_nome].has(\"acqu_effett\"):
		dati.diz_contatti[pr_nome][\"acqu_effett\"].append(pr_num)
	else:
		dati.diz_contatti[pr_nome][\"acqu_effett\"] = [pr_num]
	dati.salva_dati(\"rubrica\")

#----------------------------------
func elimina_contatto(nome_da_canc):
	dati.diz_contatti.erase(nome_da_canc)
	dati.lista_nomi.erase(nome_da_canc)
	svuota_pannello_note()
	dati.salva_dati(\"rubrica\")
	compila_rubrica()

func compila_pannello_note(nome):
	n_nome.set_text(nome)
	n_ico.select(dati.diz_contatti[nome][\"ico\"])
	n_d_nasc.set_text(dati.diz_contatti[nome][\"d_nasc\"])
	n_email.set_text(dati.diz_contatti[nome][\"mail\"])
	n_tel.set_text(dati.diz_contatti[nome][\"tel\"])
	n_note.set_text(dati.diz_contatti[nome][\"note\"])
	#------contabilita acquisti
	if dati.diz_contatti[nome].has(\"acqu_effett\"):
		var somma_acquisti = 0
		var n_acquisti     = 0
		for n_voce in dati.diz_contatti[nome][\"acqu_effett\"]:
			n_acquisti += 1
			var id_array = n_voce - 1
			somma_acquisti += dati.diz_contabilita[id_array][1][\"valore\"]
			
		$h_box/pan_note/riga3/n_acqu.text   = str(n_acquisti)
		$h_box/pan_note/riga3/tot_acqu.text = utl.rendi_decimale2str(somma_acquisti)
		if n_acquisti != 0:
			var media = somma_acquisti / float(n_acquisti)
			$h_box/pan_note/riga3/media.text = utl.rendi_decimale2str(media)
		else:
			prints(\"RUBRICA: Contabilità acquisti->\",nome,\"ha diz[acqu_effett] vuoto!\")
			$h_box/pan_note/riga3/media.text = \"0.00\"
	else:
		$h_box/pan_note/riga3/n_acqu.text   = \"0\"
		$h_box/pan_note/riga3/tot_acqu.text = \"0.00\"
		$h_box/pan_note/riga3/media.text    = \"0.00\"
	
	#------contabilita preventivi
	if dati.diz_contatti[nome].has(\"prev_effett\"):
		var in_corso = 0
		var complet  = 0
		var annull   = 0
		
		
		for cod_prev in dati.diz_contatti[nome][\"prev_effett\"]:
			var diz_prev = {}
			
			for prev in dati.lista_preventivi:
				if prev[\"codice\"] == cod_prev:
					diz_prev = prev
					break
			if   diz_prev[\"fl_effettuato\"] == 0:    in_corso += 1
			elif diz_prev[\"fl_effettuato\"] == 1:    complet  += 1
			else:                                   annull   += 1
		$h_box/pan_note/riga3/prev_in_corso.text = str(in_corso)
		$h_box/pan_note/riga3/prev_ok.text       = str(complet)
		$h_box/pan_note/riga3/prev_no.text       = str(annull)
	else:
		$h_box/pan_note/riga3/prev_in_corso.text = \"0\"
		$h_box/pan_note/riga3/prev_ok.text       = \"0\"
		$h_box/pan_note/riga3/prev_no.text       = \"0\"

func svuota_pannello_note():
	n_nome.set_text(\"\")
	n_d_nasc.set_text(\"\")
	n_email.set_text(\"\")
	n_tel.set_text(\"\")
	n_note.set_text(\"\")

func azzera_rubrica():
	dati.lista_nomi   = []
	dati.diz_contatti = {}
	dati.salva_dati(\"rubrica\")
	svuota_pannello_note()
	compila_rubrica()

func imposta_cose_modificabili(flag):
	var nodi_da_controllare = n_riga1.get_children() + n_riga2.get_children() + [n_note]
	for nodo in nodi_da_controllare:
		if nodo.has_method(\"set_editable\"):  nodo.set_editable(flag)
		if nodo.has_method(\"disabilita\"):    nodo.disabilita(!flag)
		if nodo.has_method(\"set_readonly\"):  nodo.set_readonly(!flag)
		if nodo.has_method(\"release_focus\"): nodo.release_focus()
	
	for voce in n_lista.get_children():
		voce.mouse_filter = flag
	
	if flag: n_sfondo.set_frame_color(Color(0,0.8,0,0.2))
	else:    n_sfondo.set_frame_color(Color(1,1,1,0.3))

#============= bottoni modifica rubrica =============
func imposta_btn_iniz():
	imposta_cose_modificabili(false)
	svuota_pannello_note()
	n_btn_aggiungi.show() ; n_btn_aggiungi.set_text(\"Aggiungi contatto\")
	n_btn_modifica.show() ; n_btn_modifica.set_text(\"Modifica contatto\")
	n_btn_elimina.show()
	n_btn_annulla.hide()
	n_btn_salva.hide()

func _input(event):
	if !n_note.has_focus():
		if Input.is_key_pressed(KEY_KP_ENTER) or Input.is_key_pressed(KEY_ENTER):
			if   fl_mod_aggiungi: btn_aggiungi()
			elif fl_mod_modifica: btn_salva()

func btn_salva():
	var nuovo_nome = n_nome.get_text()
	#----- contatto modificato, recupera info preventivi e acquisti
	var acquisti_tot = 0
	if dati.diz_contatti[nome_da_mod].has(\"acquisti_tot\"):
		acquisti_tot = dati.diz_contatti[nome_da_mod][\"acquisti_tot\"]
	var acquisti_num = 0
	if dati.diz_contatti[nome_da_mod].has(\"acquisti_num\"):
		acquisti_num = dati.diz_contatti[nome_da_mod][\"acquisti_num\"]
	var prev_effett  = []
	if dati.diz_contatti[nome_da_mod].has(\"prev_effett\"):
		prev_effett  = dati.diz_contatti[nome_da_mod][\"prev_effett\"]
	#----- elimina precedente e riscrivi da capo
	if n_nome.get_text() == \"\":
		pop_avviso.set_title(\"Attenzione\")
		pop_avviso.set_text(\"Campo del nome vuoto\\nImmetti un nome\")
		pop_avviso.popup()
		return
	dati.lista_nomi.erase(nome_da_mod)
	dati.diz_contatti.erase(nome_da_mod)
	fl_mod_modifica = false
	aggiungi_contatto_a_dati()
	#----- aggiungi info preventivi e acquisti recuperati
	if acquisti_num != 0:
		dati.diz_contatti[nuovo_nome][\"acquisti_tot\"] = acquisti_tot
		dati.diz_contatti[nuovo_nome][\"acquisti_num\"] = acquisti_num
	if prev_effett != []:
		dati.diz_contatti[nuovo_nome][\"prev_effett\"]  = prev_effett
	if acquisti_num != 0 or prev_effett != []:
		dati.salva_dati(\"rubrica\")
	imposta_btn_iniz()
	

func btn_aggiungi():
	if !fl_mod_aggiungi:
		fl_mod_aggiungi = true
		svuota_pannello_note()
		n_btn_annulla.show()
		n_btn_salva.hide()
		n_btn_elimina.hide()
		n_btn_modifica.hide()
		imposta_cose_modificabili(true)
		n_btn_aggiungi.set_text(\"Salva contatto\")
		set_process_input(true)
	else:
		if n_nome.get_text() == \"\":
			pop_avviso.set_title(\"Attenzione\")
			pop_avviso.set_text(\"Campo del nome vuoto\\nImmetti un nome\")
			pop_avviso.popup()
			return
		
		set_process_input(false)
		aggiungi_contatto_a_dati()
		btn_annulla()

func btn_annulla():
	fl_mod_aggiungi = false
	imposta_btn_iniz()

func btn_modifica():
	nome_da_mod = n_nome.get_text()
	if nome_da_mod == \"\":
		pop_avviso.set_title(\"Attenzione\")
		pop_avviso.set_text(\"Selezionare prima un\\ncontatto dalla rubrica\")
		pop_avviso.popup()
		return
	if !fl_mod_modifica:
		fl_mod_modifica = true
		n_btn_elimina.hide()
		n_btn_aggiungi.hide()
		n_btn_salva.show()
		n_btn_modifica.set_text(\"Annulla modifiche\")
		imposta_cose_modificabili(true)
		set_process_input(true)
	else:
		fl_mod_modifica = false
		imposta_btn_iniz()
		set_process_input(false)

func btn_elimina():
	var nome_da_canc = n_nome.get_text()
	if nome_da_canc in dati.lista_nomi:
#		pop_conferma.set_text(\"Selezionare prima un\\ncontatto dalla rubrica\")
		pop_conferma.popup()
		pop_conferma.var_argomento = nome_da_canc
	else:
		pop_avviso.set_title(\"Attenzione\")
		pop_avviso.set_text(\"Selezionare prima un\\ncontatto dalla rubrica\")
		pop_avviso.popup()"

