	for id_child in range(n_lista.get_child_count()):
		var voce = n_lista.get_child(id_child)
		voce.show()
		var voce_nome = voce.var_nome.to_lower() #voce.get_name().to_lower()
		var voce_tel  = voce.var_tel #voce.get_node(\"tel\").get_text()
		
		if testo == \"\":
			pass
		elif not ( testo in voce_nome ) and not ( testo in voce_tel ):
			voce.hide()"

