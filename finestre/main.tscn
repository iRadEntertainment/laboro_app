script/source = "#ed_nome_preventivo

extends LineEdit

onready var n_lista       = get_node(\"../../../cnt_lista_nomi/lista\")
onready var cnt_lista     = get_node(\"../../../cnt_lista_nomi\")
onready var inst_voce     = preload(\"res://finestre/voce_nome_prev.tscn\")
onready var tgg_agg_rubr  = get_node(\"../tgg_agg_rubr\")

func _ready():
	connect(\"text_entered\",self,\"_testo_cambiato\")
	connect(\"focus_entered\",self,\"_scrivi\")
	connect(\"focus_exited\",self,\"_non_scrivi\")
	yield(get_tree(),\"idle_frame\") ; compila_lista()
	cnt_lista.hide()
	aggiorna_pos_lista_nomi()

func compila_lista():
	for figlio in n_lista.get_children():
		figlio.queue_free()
	
	for chiave in dati.lista_nomi:
		var nuova_voce = inst_voce.instance()
		n_lista.add_child( nuova_voce )
		nuova_voce.set_name(chiave)
		nuova_voce.var_nome = chiave
		nuova_voce.var_tel  = dati.diz_contatti[chiave][\"tel\"]
		nuova_voce.var_mail = dati.diz_contatti[chiave][\"mail\"]
		nuova_voce.get_node(\"nome\").set_text(chiave)
		nuova_voce.get_node(\"tel\").set_text(dati.diz_contatti[chiave][\"tel\"])
		nuova_voce.hide()

func _testo_cambiato(testo):
	testo = testo.to_lower()
	mostra_voci_giuste(testo)
	release_focus()
	set_process_input(false)
	cnt_lista.hide()

func _scrivi():
	aggiorna_pos_lista_nomi()
	set_process_input(true)
	cnt_lista.show()

func _non_scrivi():
	set_process_input(false)
	cnt_lista.hide()

func _input(event):
	if event is InputEventKey:
		var testo = get_text().to_lower()
		mostra_voci_giuste(testo)
		tgg_agg_rubr.controllo_esistente()

func mostra_voci_giuste(testo):
	if testo == \"\":
		  cnt_lista.hide() ; return
	else: cnt_lista.show()
	
	for figlio in n_lista.get_children():
		figlio.hide()
	
	for figlio in n_lista.get_children():
		if testo in figlio.var_nome.to_lower():
			figlio.show()

func aggiorna_pos_lista_nomi():
	cnt_lista.set_global_position(get_global_position() + Vector2(0,rect_size.y))
	cnt_lista.rect_size.x = rect_size.x

"
