script/source = "#cnt_lista_prev

extends Container

onready var n_lista        = get_node(\"hbox/scr_lista/lista_prev\")
onready var n_comandi_prev = get_node(\"riga1/comandi_prev\")

var lista_preventivi   = []
onready var inst_voce  = preload(\"res://finestre/voce_preventivo.tscn\")

var fl_filtro_data = false
var fl_filtro_cons = false
var unix_da        = 0
var unix_a         = 0

var fl_prev_selezionato = false

func _ready():
	inizializza_filtri()
	aggiorna_lista()
	svuota_anteprima()

func inizializza_filtri():
	$riga1/filtri/op_mese_a.selected = OS.get_date()[\"month\"]-1
	$riga1/filtri/anno_a.text        = str(OS.get_date()[\"year\"])
	setta_valori_filtro_data()

func aggiorna_lista():
	for i in range($hbox/scr_lista/lista_prev.get_child_count()):
		$hbox/scr_lista/lista_prev.get_child(i).queue_free()
	
	yield(get_tree(),\"idle_frame\")
	for i in range(dati.lista_preventivi.size()):
		var dizionario = dati.lista_preventivi[i]
		if typeof(dizionario) != TYPE_DICTIONARY:
			print (\"Stucazz, non è un dizionario questa cosa nella lista_preventivi:\")
			print (dizionario)
			break
		var nuova_voce = inst_voce.instance()
		
		var data_unix = OS.get_datetime_from_unix_time(dizionario[\"data_unix\"])
		var giorno  = str(data_unix[\"day\"])
		var mese    = dati.nomi_mesi_cor[ data_unix[\"month\"] - 1 ]
		var anno    = str(data_unix[\"year\"])
		var data_da = str(giorno,\" \",mese) #,\" \",anno)
		var data_a  = dizionario[\"consegna_prev\"]
		
		n_lista.add_child( nuova_voce )
		nuova_voce.set_name(dizionario[\"codice\"])
		nuova_voce.diz_questo_prev = dizionario
		nuova_voce.id_preventivo   = i
		nuova_voce.data_da         = data_da
		nuova_voce.data_a          = data_a
		nuova_voce.aggiorna_voce_prev()
		#----- connetti segnali bottoni, per ogni nuova voce
		nuova_voce.connect(\"pressed\",self,\"_compila_anteprima\",[nuova_voce.diz_questo_prev, nuova_voce.id_preventivo])
		nuova_voce.get_node(\"hbox/btn_canc\").connect(\"pressed\",self,\"_elimina_preventivo\",[nuova_voce.id_preventivo])

func setta_valori_filtro_data():
	var diz_per_unix = { \"year\":2018, \"month\":1, \"day\":1, \"hour\":1, \"minute\":1, \"second\":1}
	diz_per_unix[\"month\"] = $riga1/filtri/op_mese_da.get_selected_id()
	diz_per_unix[\"year\"]  = int($riga1/filtri/anno_da.text)
	unix_da = OS.get_unix_time_from_datetime(diz_per_unix)
	
	if $riga1/filtri/op_mese_a.get_selected_id() in [2]:
		diz_per_unix[\"day\"]   = 28
	elif $riga1/filtri/op_mese_a.get_selected_id() in [4,6,9,11]:
		diz_per_unix[\"day\"]   = 30
	else:
		diz_per_unix[\"day\"]   = 31
	diz_per_unix[\"hour\"]  = 23
	diz_per_unix[\"month\"] = $riga1/filtri/op_mese_a.get_selected_id()
	diz_per_unix[\"year\"]  = int($riga1/filtri/anno_a.text)
	unix_a  = OS.get_unix_time_from_datetime(diz_per_unix)

func mostra_lista_filtrata():
	for voce in n_lista.get_children():
		voce.show()
		if fl_filtro_data:
			var unix_voce = voce.diz_questo_prev[\"data_unix\"]
			if not (unix_voce >= unix_da and unix_voce <= unix_a):
				voce.hide()
		if fl_filtro_cons:
			if voce.diz_questo_prev[\"fl_effettuato\"] != 0: # 0 = lavorazione in corso
				voce.hide()


#		\"codice\"        : codice_preventivo,
#		\"contatto_nome\" : n_ed_nome.text,
#		\"contatto_tel\"  : n_ed_tel.text,
#		\"data_unix\"     : OS.get_unix_time(),
#		\"consegna_prev\" : OS.get_unix_time(),
#		\"tot\"           : float(preventivo_costi[7]),
#		\"acconto\"       : float(preventivo_costi[8]),
#		\"sconto\"        : preventivo_costi[9],
#		\"costi\"         : preventivo_costi}
#		\"descrizione\"   : n_ed_descr.text,
#		\"fl_effettuato\" : 0} # 0 = in corso, 1 = effettuato, 2 = cancellato
func _compila_anteprima(diz_da_compilare, id_preventivo):
	print(\"PREVENTIVO: id preventivo in dati.lista_preventivi[id] = \"+str(id_preventivo))
	dati.fin_preventivo.fl_id_prev_anteprima = id_preventivo
	fl_prev_selezionato = true
	#--------- riga comandi prev
	n_comandi_prev.get_node(\"ed_prev_n\").text = diz_da_compilare[\"codice\"]
	n_comandi_prev.get_node(\"btn_gen_pdf\").diz_da_convertire_in_pdf = diz_da_compilare
	for figlio in n_comandi_prev.get_children():
		if figlio.has_method(\"_btn_click\"):
			figlio.disabled = false
	#--------- campi anteprima
	$hbox/prev_prew   #compila i campi dell'anteprima

func svuota_anteprima():
	fl_prev_selezionato = false
	#--------- riga comandi prev
	n_comandi_prev.get_node(\"ed_prev_n\").text        = \"codice\"
	for figlio in n_comandi_prev.get_children():
		if figlio.has_method(\"_btn_click\"):
			figlio.disabled = true
	
	#--------- campi anteprima
	

func _elimina_preventivo(id_prev):
	dati.lista_preventivi.remove(id_prev)
	dati.salva_dati(\"preventivi_lista\")
	aggiorna_lista()
