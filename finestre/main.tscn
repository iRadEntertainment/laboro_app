script/source = "#btn_set_ripristina_dati

extends Button

func _ready():
	compila_opt_btn()
	connect(\"pressed\",self,\"_click\")

func compila_opt_btn():
	var nullo        = \"\"
	var pre_apertura = 0
	var bak_manuale  = 0
	if Directory.new().file_exists(dati.perc_file_conf+\".bak\"): pre_apertura = \"dalla sessione precedente\"
	if dati.bak_manuale != \"\":                                  bak_manuale  = \"del \" + dati.bak_manuale + \" (salvataggio manuale)\"
	var opz_da_compilare = [nullo, pre_apertura, bak_manuale]
	
	$opt_btn.clear()
	for i in range(opz_da_compilare.size()):
		if not opz_da_compilare[i] in [0]: $opt_btn.add_item(opz_da_compilare[i], i)

func _click():
	var bak_da_eseguire = \"\"
	if $opt_btn.get_selected_id()   == 0: return
	elif $opt_btn.get_selected_id() == 1:
		bak_da_eseguire = \"pre-apertura\"
		get_node(\"../../pop_conferma\").set_text(\"Ripristinare i salvataggi\\ndell'ultima sessione? \")
	elif $opt_btn.get_selected_id() == 2:
		bak_da_eseguire = \"manuale\"
		get_node(\"../../pop_conferma\").set_text(\"Ripristinare i salvataggi\\ndel \"+dati.bak_manuale+\"?\")

	
	get_node(\"../..\").pop_esegui_bak(bak_da_eseguire)"

