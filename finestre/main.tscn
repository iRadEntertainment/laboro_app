onready var n_box_prev       = get_node(\"cnt_nuovo_prev/cnt_centro/scr_prev/box_prev\")
onready var pr_subtot        = get_node(\"cnt_nuovo_prev/cnt_centro/scr_prev/box_prev/riga_sub_tot/pr_subtot\")
onready var pr_tot           = get_node(\"cnt_nuovo_prev/cnt_centro/scr_prev/box_prev/riga_IVA/pr_tot\")
onready var pr_resto         = get_node(\"cnt_nuovo_prev/cnt_centro/scr_prev/box_prev/riga_acc/pr_resto\")
onready var lb_guadagno      = get_node(\"cnt_nuovo_prev/cnt_centro/scr_prev/box_prev/riga_guad/guadagno\")

onready var btn_salva_prevent= get_node(\"cnt_nuovo_prev/riga_btn/btn_salva_preventivo\")

onready var n_box_mod        = get_node(\"cnt_nuovo_prev/cnt_centro/box_mod\")

onready var n_cnt_lista_prev = get_node(\"cnt_lista_prev\")

var preventivo_costi      = []
var prossimo_prev_num     = 1

var fl_prev_per_cliente   = false setget _set_fl_prev_cliente
var fl_confez             = false
var codice_preventivo     = \"Pr-02018\"

var fl_id_prev_anteprima

func _ready():
	connetti_segnali()
	aggiorna_num_preventivo()
	compila_costi_e_opzioni()
	mostra_comandi_prev_per_cliente(fl_prev_per_cliente)
	compila_box_preventivo(\"ciao\")

func salva_diz_prezzi():
	print(\"tentativo di salvataggio diz_prezzi da fin_preventivo\")
	dati.salva_dati(\"preventivi_prezzi\")

func salva_lista_preventivi():
	print(\"tentativo di salvataggio lista_preventivi da fin_preventivo\")
	dati.salva_dati(\"preventivi_lista\")

func connetti_segnali():
	var lista_ed_stima = [n_ed_peso , n_ed_perd_mat , n_ed_tempo , n_ed_semil , n_ed_pietre , n_ed_acconto, n_ed_sconto]
	for id_ed in range (lista_ed_stima.size()):
		var ed      = lista_ed_stima[id_ed]
		ed.connect(\"focus_exited\",self,\"compila_box_preventivo\",[\"ciao\"])
		if id_ed < lista_ed_stima.size()-1:
			var ed_succ = lista_ed_stima[id_ed+1]
			ed.connect(\"text_entered\",self,\"prossimo_focus\",[ed_succ])
	n_ed_sconto.connect(\"text_entered\",self,\"compila_box_preventivo\")
	
	var lista_riga_tipo_lavoro = [op_tipo , op_diff , op_metallo , op_confez] #tgg_confez
	for op_btn in lista_riga_tipo_lavoro:
		op_btn.connect(\"item_selected\",self,\"compila_box_preventivo\")
	tgg_confez.connect(\"pressed\",self,\"compila_box_preventivo\",[\"ciao\"])

func aggiorna_num_preventivo():
	yield(get_tree(),\"idle_frame\")
	prossimo_prev_num = dati.lista_preventivi.size() + 1
	genera_codice_preventivo()
	emit_signal(\"aggiorna_tutto_fin_preventivo\")
#	n_num_prev_nuovo.aggiorna()

func check_validita_dati_contatto():
