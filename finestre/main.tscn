script/source = "#diagramma_torta

extends Container

var fl_anim_in_corso = false
var vel_anim   = 10

var valori = [10,10,20,30,1000,55,2,65,35,2]
var valori_ang      = []
var valori_ang_anim = []
var ang_maggiore_id = 0
var somma_val = 0.0

var ver1 = Color(	82/255.0,		191/255.0,		144/255.0)
var ver2 = Color(	73/255.0,		171/255.0,		129/255.0)
var ver3 = Color(	65/255.0,		152/255.0,		115/255.0)
var ver4 = Color(	57/255.0,		133/255.0,		100/255.0)
var ver5 = Color(	49/255.0,		114/255.0,		86/255.0)
var ros1 = Color(	123/255.0,		20/255.0,		24/255.0)
var ros2 = Color(	192/255.0,		31/255.0,		37/255.0)
var ros3 = Color(	218/255.0,		31/255.0,		38/255.0)
var ros4 = Color(	255/255.0,		82/255.0,		82/255.0)
var ros5 = Color(	255/255.0,		123/255.0,		123/255.0)
var blu1 = Color(	104/255.0,		147/255.0,		238/255.0)
var blu2 = Color(	134/255.0,		171/255.0,		249/255.0)
var blu3 = Color(	168/255.0,		194/255.0,		251/255.0)
var blu4 = Color(	193/255.0,		212/255.0,		251/255.0)
var blu5 = Color(	208/255.0,		223/255.0,		255/255.0)
var blu_laboro = Color(	34/255.0,		62/255.0,		102/255.0)
var gri1 = Color(	40/255.0,		39/255.0,		45/255.0)
var gri2 = Color(	87/255.0,		88/255.0,		90/255.0)
var gri3 = Color(	159/255.0,		163/255.0,		172/255.0)


var colori = [ver5, ver4, ver3, ver2, ver1, ros1, ros2, ros3, blu1, blu2, blu3, gri2]

func _ready():
	connect(\"visibility_changed\",self,\"_on_diagram_visibility_changed\")
	set_process(false)

func aggiorna_diagramma():
	fl_anim_in_corso = true
	
	var f_cont = dati.fin_contabilita
	valori = f_cont.somme_filtrate
	for i in range(valori.size()):
		valori[i] = abs(valori[i])
	
	if valori.size() == 0:
		print(\"DIAGRAMMA: nessun valore da rappresentare\")
		return
	
	somma_val = 0
	for val in valori: somma_val += val
	if somma_val == 0:
		print(\"DIAGRAMMA: somma totale uguale a zero\")
		return
	
	var angolo_iniz = 0
	var angolo_fin  = 360
	valori_ang      = []
	valori_ang_anim = []
	
	for i in range(valori.size()):
		if i != 0:
			angolo_iniz = valori_ang[i-1][1]
		angolo_fin = angolo_iniz + lerp(0, 360, valori[i]/somma_val)
		valori_ang.push_back([angolo_iniz,angolo_fin])
		valori_ang_anim.push_back(angolo_iniz)
	
	ang_maggiore_id = 0
	for i in range(1,valori.size()):
		if valori[i] > valori[ang_maggiore_id]:
			ang_maggiore_id = i
	
	set_process(true)


func _draw():
	var center = rect_size/2
	var radius = min(rect_size.x/2,rect_size.y/2)-5
	
	if somma_val == 0:
		draw_circle(center,radius, blu_laboro)
		draw_circle(center,radius*.9, Color(1,1,1,.85))
		return
	
	draw_circle(center,radius, blu_laboro)
	draw_circle(center,radius*.95, Color(1,1,1))
	for i in range(valori_ang.size()):
		draw_circle_arc(center, radius*.97, valori_ang[i][0], valori_ang_anim[i], colori[i] )
	

func _process(delta):
	if valori_ang_anim[ang_maggiore_id] >= valori_ang[ang_maggiore_id][1]-0.1:
		for i in range(valori.size()):
			valori_ang_anim[i] = valori_ang[i][1]
		
		set_process(false)
		update()
		fl_anim_in_corso = false
		return
	
	for i in range(valori.size()):
		valori_ang_anim[i] = lerp(valori_ang_anim[i], valori_ang[i][1], delta*vel_anim)
	update()

func draw_circle_arc( center, radius, angle_from, angle_anim, color ):
	var nb_points = 32
	var points_arc = []
	points_arc.push_back(center)
	var colors = [color]

	for i in range(nb_points+1):
		var angle_point = angle_from + i*(angle_anim-angle_from)/nb_points - 90
		points_arc.push_back(center + Vector2( cos( deg2rad(angle_point) ), sin( deg2rad(angle_point) ) ) * radius)
	draw_polygon(points_arc, colors)

func _on_diagram_visibility_changed():
	if fl_anim_in_corso: return
	if not is_visible_in_tree(): return
	aggiorna_diagramma()
"
