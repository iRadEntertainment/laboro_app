resource_name = "btn_abilita_contabilita"
script/source = "#btn_abilita_contabilita

extends TextureButton

var fl_tween_attivo = false

func _ready():
	connect(\"button_down\",self,\"_click\")

func _click():
	if $tim_click.is_stopped():
		$tim_click.start()
	else:
		mostra_contabilita()
		riproduci_animazione()

func riproduci_animazione():
	var tempo_tween = 0.08
	var spost = 10
	if fl_tween_attivo: return
	fl_tween_attivo = true
	$tween.stop_all()
	$tween.interpolate_property(get_parent(), \"rect_position\", get_parent().rect_position, get_parent().rect_position+Vector2(spost,0), tempo_tween, Tween.TRANS_BACK, Tween.EASE_OUT)
	$tween.start()
	yield($tween,\"tween_completed\")
	$tween.interpolate_property(get_parent(), \"rect_position\", get_parent().rect_position, get_parent().rect_position+Vector2(-spost*2,0), tempo_tween, Tween.TRANS_QUAD, Tween.EASE_IN_OUT)
	$tween.start()
	yield($tween,\"tween_completed\")
	$tween.interpolate_property(get_parent(), \"rect_position\", get_parent().rect_position, get_parent().rect_position+Vector2(spost,0), tempo_tween, Tween.TRANS_BACK, Tween.EASE_OUT)
	$tween.start()
	yield($tween,\"tween_completed\")
	fl_tween_attivo = false

func mostra_contabilita():
	dati.fl_cont_vis = not dati.fl_cont_vis"

